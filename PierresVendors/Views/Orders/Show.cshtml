@{
  Layout = "_Layout";
}
<div class = "container">
  <div class = "jumbotron">
    <h1>Order Details for @Model["vendor"].VendorName</h1>
    <h2>@Model["order"].Title</h2>
    <h2>@Model["order"].Description</h2>
    <h2>@Model["order"].Price</h2>
    <h2>@Model["order"].Date</h2>
    <a href='/vendors/@Model["vendor"].Id/orders/new'>Add another order for this Vendor</a>
    <a href='/vendors'>View all vendor</a>
    </div>
</div>

