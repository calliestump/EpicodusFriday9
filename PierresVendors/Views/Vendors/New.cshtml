@{
  Layout = "_Layout";
}
<div class = "container">
  <div class = "jumbotron">
    <h1><u>Vendors Form</u></h1><br>
    <p>Add a Vendor here</p>
    <form action="/vendors" method="post">
      <label for="vendorName">Vendors Name:</label><br>
      <input id="vendorName" name="vendorName"><br><br>
      <label for="vendorDescription">Vendors Description:</label><br>
      <input id="vendorDescription" name="vendorDescription"><br><br>
      <button type="submit">Add Vendor</button>
    </form>
  </div>
</div>
